{
  "name": "taksu",
  "version": "2.1.1",
  "description": "A controlled-entropy toolkit for modern JavaScript developers.",
  "keywords": [
    "random",
    "utility",
    "toolkit",
    "javascript"
  ],
  "homepage": "https://github.com/fauziralpiandi/taksu#readme",
  "bugs": {
    "url": "https://github.com/fauziralpiandi/taksu/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fauziralpiandi/taksu.git"
  },
  "license": "MIT",
  "author": "Fauzira Alpiandi <fwzyrln@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && gzip -c dist/index.mjs | wc -c",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "22.15.18",
    "@typescript-eslint/eslint-plugin": "8.32.1",
    "@typescript-eslint/parser": "8.32.1",
    "@vitest/coverage-v8": "3.1.3",
    "eslint": "9.26.0",
    "prettier": "3.5.3",
    "tsup": "8.5.0",
    "typescript": "5.8.3",
    "vitest": "3.1.3"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  }
}
